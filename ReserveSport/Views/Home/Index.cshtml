            <!-- Text -->

            <hr class="my-1">
            <p class="lead text-info font size-font text-center">محمدرضا موحدی جم</p>

            <!-- Button -->
            <button class="btn btn-sm bg-warning btn-block aqua-gradient font waves-effect waves-light">مشاهده مجموعه ورزشی</button>

        </div>

    </div>
    <!-- Card -->
</div>
